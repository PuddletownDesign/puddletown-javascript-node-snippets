const http = require('http');

const port = 3000;

const requestHandler = (req, res) => {
 console.log(req.url);
}
const server = http.createServer(requestHandler);

server.listen(port, (err) => {
 if (err) {
   return console.log(err);
 }
 console.log(`server is running on ${port}`);
})
$0
